#!/usr/bin/python

import sys, os
from subprocess import * 

sessionName = 'CS.'+sys.argv[1]

out = os.popen("screen -list")            
for line in out:
    #print '>', line,
    if sessionName in line :
        print "screen -d -r", line.split()[0]
        out.close()
        sys.exit(0)
    
out.close()
print "screen -S " + sessionName



